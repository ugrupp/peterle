{
  "name": "peterle",
  "version": "2.0.0",
  "description": "Website for Hotel & Restaurant Peterle",
  "repository": "ugrupp/peterle",
  "author": "Urs Grupp, 21sieben",
  "homepage": "https://www.hotel-peterle.de/",
  "license": "GPL-3.0",
  "scripts": {
    "build": "npm run build:hugo && npm run build:vite",
    "build:hugo": "hugo -d ../dist -s site -v --cleanDestinationDir",
    "build:vite": "vite build",
    "dev": "run-p dev:**",
    "dev:vite": "npm run build:hugo && vite --host",
    "dev:hugo-watch": "onchange 'site/content/**/*' 'site/config.toml' 'site/layouts/**/*' 'site/static/**/*' -- npm run build:hugo"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "colcade": "^0.2.0",
    "eslint": "^8.23.1",
    "eslint-config-google": "^0.14.0",
    "fontfaceobserver": "^2.0.13",
    "headroom.js": "^0.9.4",
    "hugo-bin": "^0.91.1",
    "lazysizes": "^5.2.1",
    "lodash.throttle": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "sass": "^1.54.9",
    "scrollreveal": "^4.0.0-beta.31",
    "smooth-scroll": "^16.1.3",
    "stylelint": "^14.12.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-standard": "^28.0.0",
    "swiper": "^4.3.0",
    "vite": "^3.1.3",
    "vite-hugo-plugin": "^3.0.5",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^3.0.7",
    "vite-plugin-svg-sprite-component": "^1.0.10"
  },
  "browserslist": [
    "ie >= 11",
    "ie_mob >= 11",
    "ff >= 30",
    "chrome >= 34",
    "safari >= 7",
    "opera >= 23",
    "ios >= 7",
    "android >= 4.4",
    "bb >= 10"
  ]
}
